{
  "openapi": "3.1.0",
  "info": {
    "title": "CaptureOnePresetBaker",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://capture-one-preset-bot.onrender.com"
    }
  ],
  "paths": {
    "/analyze": {
      "post": {
        "operationId": "analyze",
        "summary": "Analyze image and return settings",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": { "type": "string", "format": "binary" }
                },
                "required": ["file"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "JSON with slider settings",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {}
                }
              }
            }
          }
        }
      }
    },
    "/generate": {
      "post": {
        "operationId": "generate",
        "summary": "Generate .costyle file from settings",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "settings": { "type": "object" },
                  "album": { "type": "string" }
                },
                "required": ["settings", "album"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Return .costyle file",
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    }
  }
}
