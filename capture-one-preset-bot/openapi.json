{
  "openapi": "3.0.1",
  "info": {
    "title": "CaptureOnePresetBaker",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://2451-174-55-135-163.ngrok-free.app"
    }
  ],
  "paths": {
    "/analyze": {
      "post": {
        "summary": "Analyze image and return settings",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": { "type": "string", "format": "binary" }
                },
                "required": ["file"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "JSON with slider settings",
            "content": {
              "application/json": {
                "schema": { "type": "object" }
              }
            }
          }
        }
      }
    },
    "/generate": {
      "post": {
        "summary": "Generate .costyle file from settings",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "settings": { "type": "object" },
                  "album": { "type": "string" }
                },
                "required": ["settings", "album"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Return .costyle file",
            "content": {
              "application/octet-stream": {
                "schema": { "type": "string", "format": "binary" }
              }
            }
          }
        }
      }
    }
  }
}
